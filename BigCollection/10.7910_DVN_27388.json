","multiple":true,"typeClass":"compound","value":[{"distributorName":{"typeName":"distributorName","multiple":false,"typeClass":"primitive","value":"Harvard Dataverse Network"},"distributorURL":{"typeName":"distributorURL","multiple":false,"typeClass":"primitive","value":"http://thedata.harvard.edu/dvn/"},"distributorLogoURL":{"typeName":"distributorLogoURL","multiple":false,"typeClass":"primitive","value":"http://thedata.harvard.edu/dvn/resources/images/dvnPoweredByLogo.gif"}}]},{"typeName":"distributionDate","multiple":false,"typeClass":"primitive","value":"2014-09-23"},{"typeName":"dateOfDeposit","multiple":false,"typeClass":"primitive","value":"2014-09-23"}]}},"files":[{"description":"","label":"Dataset_HealthBehavAcadPerfAffect_23.09.14.tab","restricted":false,"version":1,"datasetVersionId":60276,"dataFile":{"id":2499805,"persistentId":"doi:10.7910/DVN/27388/SPWIGT","pidURL":"https://doi.org/10.7910/DVN/27388/SPWIGT","filename":"Dataset_HealthBehavAcadPerfAffect_23.09.14.tab","contentType":"text/tab-separated-values","filesize":92624,"description":"","storageIdentifier":"s3://dvn-cloud:233490","originalFileFormat":"application/x-spss-sav","originalFormatLabel":"SPSS SAV","UNF":"UNF:5:2iitGmaN16nWAn5TTjQ+xg==","rootDataFileId":-1,"md5":"e46177c6fd4c23b9b788dddf5624d384","checksum":{"type":"MD5","value":"e46177c6fd4c23b9b788dddf5624d384"}}},{"description":"Information for dataset","label":"README.rtf","restricted":false,"version":1,"datasetVersionId":60276,"dataFile":{"id":2499841,"persistentId":"doi:10.7910/DVN/27388/U8IPJZ","pidURL":"https://doi.org/10.7910/DVN/27388/U8IPJZ","filename":"README.rtf","contentType":"text/plain; charset=US-ASCII","filesize":1980,"description":"Information for dataset","storageIdentifier":"s3://dvn-cloud:233530","rootDataFileId":-1,"md5":"fe996b088b26771a18baeeef464b6cd1","checksum":{"type":"MD5","value":"fe996b088b26771a18baeeef464b6cd1"}}}]}}}