he <a href=\"http://ebrary.ifpri.org/utils/getfile/collection/p15738coll2/id/70377/filename/70321.pdf\">Estimation of a Regionalized Mexican Social Accounting Matrix: Using Entropy Techniques to Reconcile Disparate Data Sources</a>\r\n\r\n\r\n"}}]},{"typeName":"subject","multiple":true,"typeClass":"controlledVocabulary","value":["Arts and Humanities","Social Sciences"]},{"typeName":"keyword","multiple":true,"typeClass":"compound","value":[{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"social accounting matrix"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"STW Thesaurus for Economics"},"keywordVocabularyURI":{"typeName":"keywordVocabularyURI","multiple":false,"typeClass":"primitive","value":" http://zbw.eu/stw/descriptor/19237-2"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"SAM"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"agricultural production"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"Mexico"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"AGROVOC"},"keywordVocabularyURI":{"typeName":"keywordVocabularyURI","multiple":false,"typeClass":"primitive","value":"http://aims.fao.org/aos/agrovoc/c_4792"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"North America"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"AGROVOC"},"keywordVocabularyURI":{"typeName":"keywordVocabularyURI","multiple":false,"typeClass":"primitive","value":"http://aims.fao.org/aos/agrovoc/c_5219"}}]},{"typeName":"topicClassification","multiple":true,"typeClass":"compound","value":[{"topicClassValue":{"typeName":"topicClassValue","multiple":false,"typeClass":"primitive","value":"Social accounting 