alue":[{"otherGeographicCoverage":{"typeName":"otherGeographicCoverage","multiple":false,"typeClass":"primitive","value":"Please see geographic coverage."}},{"otherGeographicCoverage":{"typeName":"otherGeographicCoverage","multiple":false,"typeClass":"primitive","value":"Georgia"}},{"otherGeographicCoverage":{"typeName":"otherGeographicCoverage","multiple":false,"typeClass":"primitive","value":"Texas"}},{"otherGeographicCoverage":{"typeName":"otherGeographicCoverage","multiple":false,"typeClass":"primitive","value":"United States"}}]}]},"socialscience":{"displayName":"Social Science and Humanities Metadata","fields":[{"typeName":"universe","multiple":true,"typeClass":"primitive","value":["All prisoners in Georgia and Texas eligible for parole who were not returning to unsampled rural counties, who were not returning to live outside of the state, and who did not have existing warra\nnts or detainers against them."]},{"typeName":"samplingProcedure","multiple":false,"typeClass":"primitive","value":"Stratified random sampling was employed to obtain 2,007  prisoner participants in Georgia and 1,975 prisoner participants in  Texas, who were then assigned to one of six experimental and control groups  when released from prison. The experiment lasted one year beyond that date."},{"typeName":"cleaningOperations","multiple":false,"typeClass":"primitive","value":"Checked for undocumented or out-of-range codes."}]}},"files":[{"description":"","label":"Georgia Released Inmate Data","restricted":false,"version":1,"datasetVersionId":42127,"dataFile":{"id":2664758,"persistentId":"","pidURL":"","filename":"Georgia Released Inmate Data","contentType":"data/various-formats","filesize":0,"description":"","storageIdentifier":"","rootDataFileId":-1,"md5":"","checksum":{"type":"MD5","value":""}}},{"description":"","label":"Texas Released Inmate Data","restricted":false,"version":1,"datasetVersionId":42127,"dataFile":{"id":2664757,"persistentId":"","pidURL":"","filename":"Texas Released In