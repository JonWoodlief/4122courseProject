:{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Oxford"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Fraile, Marta"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Spanish Scientific Research Institute"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Hahn, Kyu S."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Seoul National University"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Hansen, Kasper M."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Copenhagen"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Harell, Allison"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Université du Québec à Montréal"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Helbling, Marc"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Bamberg"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Jackman, Simon D."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"US Studies Centre, University of Sydney"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Kobayashi, Tetsuro"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"City University of Hong Kong"}}]},{"typeName":"datasetContact","multiple":true,"typeClass":"compound","value":[{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Soroka, Stuart"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"University of Michigan"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"ssoroka@umich.edu"}}]},{"typeName":"dsDescription","multiple":true,"typeClass":"compound","value":[{"dsDescriptionValue":{"typeName":"dsDescriptionValue","multiple":false,"typeClass":"primitive","value":"Employing a comparative experimental design drawing on over 18,000 interviews across 11 countries on 4 continents, we revisit the discussion about economic and cultural drivers of attitudes towards immigrants in advanced democracies. We manipulate the occupational status, skin tone and national origin of immigrants in short vignettes. Results are most consistent with a sociotropic economic threat thesis: higher-skilled immigrants are preferred to lower-skilled in all countries, and at all levels of native SES. We find, in contrast, little support for the labor market competition hypothesis, since respondents are not more opposed to immigrants in their own SES stratum. While skin tone itself has little effect in any country, immigrants from Muslim majority countries do elicit significantly lower levels of support, and racial animus remains a powerful force. "}}]},{"typeName":"subject","multiple":true,"typeClass":"controlledVocabulary","value":["Social Sciences"]},{"typeName":"keyword","multiple":true,"typeClass":"compound","value":[{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"immigration"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"public opinion"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"survey-based experiments"}}]},{"typeName":"depositor","multiple":false,"typeClass":"primitive","value":"Soroka, Stuart"},{"typeName":"dateOfDeposit","multiple":false,"typeClass":"prim