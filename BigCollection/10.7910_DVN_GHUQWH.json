{"status":"OK","data":{"id":48424,"identifier":"DVN/GHUQWH","persistentUrl":"https://doi.org/10.7910/DVN/GHUQWH","protocol":"doi","authority":"10.7910","publisher":"Harvard Dataverse","publicationDate":"2013-03-12","storageIdentifier":"s3://dvn-cloud:1902.1/20483","latestVersion":{"id":45360,"storageIdentifier":"s3://dvn-cloud:1902.1/20483","versionNumber":2,"versionMinorNumber":0,"versionState":"RELEASED","versionNote":"","distributionDate":"2012","productionDate":"Production Date","lastUpdateTime":"2013-03-13T03:45:00Z","releaseTime":"2013-03-12T00:00:00Z","createTime":"2013-03-13T03:45:00Z","license":"CC0","termsOfUse":"CC0 Waiver","metadataBlocks":{"citation":{"displayName":"Citation Metadata","fields":[{"typeName":"title","multiple":false,"typeClass":"primitive","value":"Effects of hurricane Felix on bat communities that disperse tree seeds in the North Atantic Autonomous Region of Nicaragua"},{"typeName":"author","multiple":true,"typeClass":"compound","value":[{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Guillén, Delvis"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Tropical Agricultural Research and Higher Education Center"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Bryan Finegan"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Tropical Agricultural Research and Higher Education Center"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Yadid Ordoñez"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Tropical Agricultural Research and Higher Education Center"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Diego Delgado"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Tro