ican Political Studies at Harvard University, Institutional Development Initiative at Harvard University, and Harvard University Library.</p>"}}]},{"typeName":"subject","multiple":true,"typeClass":"controlledVocabulary","value":["Social Sciences"]},{"typeName":"keyword","multiple":true,"typeClass":"compound","value":[{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"committed"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"concurred"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"read"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"received"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"received"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"sent"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"action"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"Manuscript"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"prayer-format"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"Several names from a committee"},"keywordVocabulary":{"typeName":"keyword