{"status":"OK","data":{"id":3333323,"identifier":"DVN/3EDO2Y","persistentUrl":"https://doi.org/10.7910/DVN/3EDO2Y","protocol":"doi","authority":"10.7910","publisher":"Harvard Dataverse","publicationDate":"2019-01-28","storageIdentifier":"s3://10.7910/DVN/3EDO2Y","latestVersion":{"id":148671,"storageIdentifier":"s3://10.7910/DVN/3EDO2Y","versionNumber":1,"versionMinorNumber":0,"versionState":"RELEASED","productionDate":"Production Date","UNF":"UNF:6:mA5OK5NyEVzNksUbpXX5kQ==","lastUpdateTime":"2019-01-28T18:41:15Z","releaseTime":"2019-01-28T18:41:15Z","createTime":"2018-12-19T15:40:14Z","license":"CC0","termsOfUse":"CC0 Waiver","metadataBlocks":{"citation":{"displayName":"Citation Metadata","fields":[{"typeName":"title","multiple":false,"typeClass":"primitive","value":"Replication Data for: Overseas Credit Claiming and Domestic Support for Foreign Aid"},{"typeName":"author","multiple":true,"typeClass":"compound","value":[{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Dietrich, Simone"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Geneva"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Hyde, Susan D."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of California, Berkeley"},"authorIdentifierScheme":{"typeName":"authorIdentifierScheme","multiple":false,"typeClass":"controlledVocabulary","value":"ORCID"},"authorIdentifier":{"typeName":"authorIdentifier","multiple":false,"typeClass":"primitive","value":"0000-0003-4079-598X"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Winters, Matthew"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Illinois at Urbana-Champaign"},"authorIdentifierScheme":{"typeName":"authorIdentifierScheme","multiple":false,