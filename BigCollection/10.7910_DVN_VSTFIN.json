{"status":"OK","data":{"id":3184430,"identifier":"DVN/VSTFIN","persistentUrl":"https://doi.org/10.7910/DVN/VSTFIN","protocol":"doi","authority":"10.7910","publisher":"Harvard Dataverse","publicationDate":"2018-07-09","storageIdentifier":"s3://10.7910/DVN/VSTFIN","latestVersion":{"id":139883,"storageIdentifier":"s3://10.7910/DVN/VSTFIN","versionNumber":1,"versionMinorNumber":1,"versionState":"RELEASED","productionDate":"Production Date","lastUpdateTime":"2018-07-25T19:25:10Z","releaseTime":"2018-07-25T19:25:10Z","createTime":"2018-07-25T19:25:04Z","license":"CC0","termsOfUse":"CC0 Waiver","metadataBlocks":{"citation":{"displayName":"Citation Metadata","fields":[{"typeName":"title","multiple":false,"typeClass":"primitive","value":"Replication Data for: Investing Cash Transfers to Raise Long-Term Living Standards"},{"typeName":"author","multiple":true,"typeClass":"compound","value":[{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Gertler, Paul J."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of California, Berkeley"},"authorIdentifierScheme":{"typeName":"authorIdentifierScheme","multiple":false,"typeClass":"controlledVocabulary","value":"ORCID"},"authorIdentifier":{"typeName":"authorIdentifier","multiple":false,"typeClass":"primitive","value":"0000-0003-0280-7513"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Martinez, Sebastian W."},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Inter-American Development Bank"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Rubio-Codina, Marta"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Inter-American Development Bank"}}]},{"typeName":"datasetContact","multiple":true,"typeClass":"compound","value":[{"datasetContactName":{"type