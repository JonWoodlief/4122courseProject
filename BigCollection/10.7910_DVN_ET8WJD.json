ffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"IPA"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Pimkina, Svetlana"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"The World Bank"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Sen, Iman"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"The World Bank"}}]},{"typeName":"datasetContact","multiple":true,"typeClass":"compound","value":[{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Buchmann, Nina"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"Stanford University"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"ncb21@duke.edu"}},{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Field, Erica"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"Duke University"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"emf23@duke.edu"}},{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Glennerster, Rachel"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"Department for International Development"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"rglenner@mit.edu"}},{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Nazneen, Shahana"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"IPA"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"shahanans@hotmail.com"}},{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Pimkina, Svetlana"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"The World Bank"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"svetlana.pimkina@duke.edu"}},{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Sen, Iman"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"The World Bank"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"iman.sen@gmail.com"}}]},{"typeName":"dsDescription","multiple":true,"typeClass":"compound","value":[{"dsDescriptionValue":{"typeName":"dsDescriptionValue","multiple":false,"typeClass":"primitive","value":"A clustered randomized trial in Bangladesh examines alternative strategies to reduce child marriage and teenage childbearing and increase girl's education. Communities were randomized into three treatment and one control group in a 2:1:1:2 ratio. From 2008, girls in treatment communities received either i) a six-month empowerment program, ii) a financial incentive to delay marriage, or iii) empowerment plus incentive. Data from 15,739 girls 4.5 years after program completion show that girls eligible for the incentive for at least two years were less likely to be married under 18, less likely to have given birth under 20, and  more likely to be in school at age 22. Unlike other incentive programs that are conditional on girls staying in school, an incentive conditional on marriage alone has the potential to benefit out-of-school girls. We find insignificantly different effects for girls in and out of school at baseline. The empowerment program did not decrease child marriage or teenage childbearing. However, girls eligible for the empowerment program were more likely to be in-school."}}]},{"typeName":"subject","multiple":true,"typeClass":"controlledVocabulary","value":["Social Sciences"]},{"typeName":"depositor","multiple":false,"typeClass":"primitive","value":"Rubio, Karl"},{"typeName":"dateOfDeposit","multiple":false,"typeClass":"primitive","value":"2018-06-27"}]}},"files":[{"label":"Bangladesh_child_marriage.zip","restricted":false,"version":1,"datasetVersionId":139871,"dataFile":{"id":3181176,"persistentId":"doi:10.7910/DVN/ET8WJD/GY5RD8","pidURL":"https://doi.org/10.7910/DVN/ET8WJD/GY5RD8","filename":"Bangladesh_child_marriage.zip","contentType":"application/zip","filesize":37759580,"storageIdentifier":"s3://dvn-cloud:164428bb870-43cd0cb28b16","rootDataFileId":-1,"md5":"a1b476c1f3ede69afadf4e406a036d48","checksum":{"type":"MD5","value":"a1b476c1f3ede69afadf4e406a036d48"}}}]}}}