elds":[{"typeName":"unitOfAnalysis","multiple":true,"typeClass":"primitive","value":["individual"]},{"typeName":"universe","multiple":true,"typeClass":"primitive","value":["In all, Eurobarometer 75.2 interviewed 26,825 citizens in the 27 countries of the European Union after the 2004/2007 enlargement (i.e. including the Accession Countries Romania and Bulgaria). All respondents were residents in their respective country, nationals and non-nationals but EU-citizens, and aged 15 and over. Respondents were expected to have sufficient command of one of the respective national language(s) to answer the questionnaire."]},{"typeName":"samplingProcedure","multiple":false,"typeClass":"primitive","value":"Multistage national probability samples."},{"typeName":"collectionMode","multiple":false,"typeClass":"primitive","value":"face-to-face interview"},{"typeName":"weighting","multiple":false,"typeClass":"primitive","value":"Please review the ICPSR codebook; \"Weighting Information\" section for information concerning individual weights and the \"Technical Specifications\" section for general weighting information."},{"typeName":"cleaningOperations","multiple":false,"typeClass":"primitive","value":"Checked for undocumented or out-of-range codes."}]}},"files":[{"description":"","label":"Eurobarometer 75.2 Economic Crisis, Volunteer Work, the Environment, Audiovisual Interests, and Helplines for Social Services, April-May 2011","restricted":false,"version":1,"datasetVersionId":38666,"dataFile":{"id":2639499,"persistentId":"","pidURL":"","filename":"Eurobarometer 75.2 Economic Crisis, Volunteer Work, the Environment, Audiovisual Interests, and Helplines for Social Services, April-May 2011","contentType":"data/various-formats","filesize":0,"description":"","storageIdentifier":"","rootDataFileId":-1,"md5":"","checksum":{"type":"MD5","value":""}}}]}}}